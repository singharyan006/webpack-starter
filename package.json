{
  "name": "webpack-template",
  "version": "1.0.0",
  "description": "A modern Webpack starter template with Babel, ESLint, and Prettier",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "webpack --mode production",
    "lint": "eslint src --ext .js",
    "format": "prettier --write \"src/**/*.{js,css,html,json}\""
  },
  "keywords": ["webpack", "template", "starter", "babel", "eslint", "prettier"],
  "author": "Aryan Singh",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@eslint/js": "^9.30.1",
    "@eslint/json": "^0.12.0",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^16.3.0",
    "html-webpack-plugin": "^5.6.3",
    "prettier": "^3.6.2",
    "style-loader": "^4.0.0",
    "webpack": "^5.100.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
